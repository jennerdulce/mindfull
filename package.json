  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jennerdulce/mindfull/issues"
  },
  "homepage": "https://github.com/jennerdulce/mindfull#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.0",
    "colors": "^1.4.0",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.6.0",
    "mongoose": "^7.3.1",
    "react-reveal": "^1.2.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
